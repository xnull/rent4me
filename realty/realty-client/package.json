    "jquery": "~2.1.3",
    "underscore": "~1.7.0",
    "validator.js": "~1.0.1",
    "JSON2": "0.1.0",
    "dropzone": "3.12.0",
    "reactify": "~0.17.1",
    "jstransform": "~8.2.0",
    "react-tools": "~0.12.2",
    "through": "~2.3.6",
    "flux": "~2.0.1",
    "events": "~1.0.2",
    "object-assign": "~2.0.0",
    "moment": "2.8.4",
    "accounting": "0.4.1",
    "sockjs-client": "1.0.0-beta.12",
    "websocket-multiplex": "0.1.0"
  },
  "devDependencies": {
    "babel-core": "5.6.18",
    "babel-loader": "5.3.1",
