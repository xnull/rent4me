{
    "name": "realty-client",
    "version": "1.0.0",
    "description": "The realty project",
    "author": "bynull",
    "scripts": {},

    "browser": {
      "bootstrap": "./node_modules/bootstrap/dist/js/bootstrap.js",
      "blockui": "./node_modules/bower/blockui/jquery.blockUI.js",
      "jquery": "./node_modules/jquery/dist/jquery.js"
    },
    "jquery": {
      "exports": "$"
    },
    "blockui": {
      "exports": "blockui",
      "depends": {
        "jquery": "$"
      }
    },

    "browserify": {
      "transform": [
        "browserify-shim"
        ]
    },
    "browserify-shim": {
        "bootstrap": {
            "depends": [
                "jquery:$"
            ]
        }
    },
    "bootstrap": {
        "exports": "bootstrap",
        "depends": {
            "jquery": "$"
        }
    },


    "dependencies": {
        "bower": "~1.3.8",

        "gulp": "~3.8.5",
        "gulp-jshint": "~1.6.4",
        "gulp-print": "~1.1.0",
        "gulp-concat": "~2.2.0",
        "gulp-uglify": "~0.3.1",
        "gulp-clean": "~0.3.1",
        "gulp-bower-files": "~0.2.6",
        "gulp-minify-css": "~0.3.10",
        "gulp-open": "~0.2.8",
        "gulp-util": "~3.0.1",
        "vinyl-source-stream": "~1.0.0",
        "gulp-webserver": "~0.8.1",
        "gulp-ng-html2js": "~0.1.8",
        "gulp-watch": "~1.0.7",
        "gulp-livereload": "~2.1.1",

        "browserify": "~6.3.2",
        "browserify-shim": "~3.8.0",
        "watchify": "~1.0.6",

        "bootstrap": "~3.2.0",
        "jquery": "~2.1.1",
        "underscore": "~1.7.0",
        "validator.js": "~1.0.1",
        "JSON2": "0.1.0",
        "dropzone": "3.12.0",

        "react": "~0.12.1",
        "react-router": "~0.11.1",
        "reactify": "~0.17.1",
        "jstransform": "~8.2.0",
        "react-tools": "~0.12.1",
        "through": "~2.3.6",

        "flux": "~2.0.1",
        "events": "~1.0.2",
        "object-assign": "~2.0.0"
    },
    "devDependencies": {
        "karma": "~0.10",
        "karma-jasmine": "~0.1.5",
        "karma-chrome-launcher": "~0.1.4",
        "karma-phantomjs-launcher": "~0.1.4",
        "protractor": "~0.20.1",
        "karma-junit-reporter": "^0.2.2",
        "shelljs": "^0.2.6",
        "http-server": "^0.6.1"
    }
}
