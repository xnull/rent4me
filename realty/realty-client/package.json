    "gulp-concat": "~2.2.0",
    "gulp-uglify": "~0.3.1",
    "gulp-substituter": "~0.3.0",
    "yargs": "~3.7.2",
    "gulp-clean": "~0.3.1",
    "gulp-bower-files": "~0.2.6",
    "gulp-minify-css": "~0.3.10",
