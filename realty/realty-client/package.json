{
  "name": "realty-client",
  "version": "1.0.0",
  "description": "The realty project",
  "author": "bynull",
  "scripts": {
    "build": "webpack --progress --profile --colors",
    "watch": "webpack-dev-server --hot --inline --progress --colors",
    "test": "echo \"No unit tests and e2e tests yet\" && exit 1"
  },
  "browser": {
    "bootstrap": "./node_modules/bootstrap/dist/js/bootstrap.js",
    "jquery": "./node_modules/jquery/dist/jquery.js",
    "rent4meUtil": "./app/shared/common/Utils.js",
    "rent4meComponents": "./app/shared/common/AppComponents.js",
    "rent4meCookies": "./app/shared/common/Cookies.js",
    "rent4meAjax": "./app/shared/common/Ajax.js",
    "rent4meEmitter": "./app/shared/common/Emitter.js",
    "rent4meBlockUI": "./app/shared/common/BlockUI.js",
    "rent4meAutocomplete": "./app/shared/common/Autocomplete.js",
    "skel": "./node_modules/bower/skel-old/src/skel.js"
  },
  "jquery": {
    "exports": "$"
  },
  "browserify": {
    "transform": [
      "browserify-shim"
    ]
  },
  "browserify-shim": {
    "bootstrap": {
      "depends": [
        "jquery:$"
      ]
    },
    "external": "global:External"
  },
  "bootstrap": {
    "exports": "bootstrap",
    "depends": {
      "jquery": "$"
    }
  },
